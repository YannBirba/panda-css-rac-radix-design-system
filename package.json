{
  "name": "design-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prepare": "panda codegen",
    "build": "tsc && vite build",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,scss,json} --config ./.prettierrc.yaml",
    "clean": "pnpm format && pnpm lint",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:ci": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "pnpm build && vite preview",
    "coverage": "pnpm build && vitest run --coverage",
    "test": "pnpm build && vitest",
    "test:ui": "pnpm build && vitest --ui",
    "test:ci": "npm run build && vitest run",
    "e2e": "pnpm exec playwright test",
    "e2e:ui": "pnpm exec playwright test --ui",
    "e2e:codegen": "pnpm exec playwright codegen",
    "e2e:report": "pnpm exec playwright show-report",
    "check": "pnpm clean && pnpm test && pnpm e2e",
    "check:ci": "pnpm clean && pnpm test:ci && pnpm e2e"
  },
  "dependencies": {
    "@radix-ui/react-accessible-icon": "^1.0.3",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.1.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-menubar": "^1.0.3",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-toolbar": "^1.0.4",
    "canvas-confetti": "^1.6.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-aria-components": "1.0.0-alpha.5",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.9.0",
    "@playwright/test": "^1.36.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/canvas-confetti": "^1.6.0",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-formatter-github": "^1.1.4",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-array-func": "^3.1.8",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-perfectionist": "^1.5.1",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-form-fields": "^1.2.22",
    "eslint-plugin-react-hook-form": "^0.3.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-perf": "^3.3.1",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-ssr-friendly": "^1.2.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-validate-jsx-nesting": "^0.1.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  }
}
